class Solution {
public:
    bool isAnagram(string s, string t) {
        unordered_map<int, int> countS;
        unordered_map<int, int> countT;
        if(s.size() != t.size()){
            return false;
        }
        for(int i = 0; i < s.size(); i++ ){
            countS[s[i]]++;
            countT[t[i]]++;
            
        }
        return countT == countS;
        
    }
};
